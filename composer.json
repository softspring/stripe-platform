{
    "name": "softspring/stripe-platform",
    "description": "This bundle provides customer features.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "softspring/platform-bundle": "^1.0@dev",
        "stripe/stripe-php": "^7.31"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\PlatformBundle\\Stripe\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\PlatformBundle\\Stripe\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "conflict": {
        "stripe/stripe-php": "<6.40.0"
    },
    "require-dev": {
        "symfony/http-kernel": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "phpunit/phpunit": "^9.1",
        "doctrine/common": "^2.12",        
        "softspring/doctrine-templates": "^1.1",
        "softspring/customer-bundle": "^1.1@dev"
    }
}
